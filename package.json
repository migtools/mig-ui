    "@babel/preset-typescript": "7.3.3",
    "@emotion/babel-preset-css-prop": "10.0.9",
    "@emotion/styled": "10.0.9",
    "@types/node": "11.11.4",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
