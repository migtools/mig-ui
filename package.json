    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "styled-system": "4.0.5",
    "ts-loader": "5.3.3",
    "tslint": "5.14.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.4000",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
