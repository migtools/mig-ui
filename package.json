    "process": "^0.11.10",
    "q": "^1.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-hot-loader": "^4.13.0",
    "react-json-view": "^1.21.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^2.4.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "sass": "^1.22.10",
    "simple-oauth2": "^4.1.0",
    "stream": "^0.0.2",
    "tree-crawl": "^1.0.5",
    "uuid": "^3.3.2"
  }
}
